    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "helmet": "^3.9.0",
    "java-properties": "^1.0.0-0",
    "joi": "^14.3.0",
    "mongoose": "^5.7.5",
    "node-fetch": "^2.5.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "saslprep": "^1.0.3",
    "ts-node": "^6.0.3",
    "util": "^0.11.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.7",
