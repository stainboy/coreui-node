    "chai": "^4.2.0",
    "jsdoc": "^3.5.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.10",
    "pkg": "^4.3.7",
    "source-map-loader": "^0.2.4",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.2.7",
