    "@types/connect-redis": "^0.0.9",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^6.1.0",
    "@types/ejs": "^2.6.1",
    "@types/express": "^4.11.0",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "^0.0.42",
    "@types/joi": "^14.0.1",
    "@types/mongoose": "^5.3.27",
    "@types/node": "^10.12.18",
