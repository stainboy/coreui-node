{
  "name": "scaffold-node",
  "version": "0.0.1",
  "bin": "./build/config/server/index.js",
  "description": "This generator will help you to build your own Node.js Express Mongodb API using TypeScript.",
  "author": "miles.chen01@sap.com",
  "license": "MIT",
  "homepage": "https://github.wdf.sap.corp/AWEval/scaffold-node",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "nodemon",
    "test": "NODE_ENV=test mocha -r ts-node/register test/index.js --exit",
    "swagger": "swagger-jsdoc -d swaggerDef.js -o ./build/swagger.json",
    "package": "pkg --out-path=./release .",
    "release": "run-s build swagger package"
  },
  "files": [
    "src"
  ],
  "pkg": {
    "assets": [
      "build/*.json",
      "node_modules/swagger-ui-dist/*"
    ],
    "targets": [
      "node10"
    ]
  },
  "dependencies": {
